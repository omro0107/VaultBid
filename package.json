{
  "name": "vaultbid",
  "version": "1.0.0",
  "description": "A project for an auction house website",
  "author": "Oda Rosenkilde <odaros00183@stud.noroff.no",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "devDependencies": {
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.15.0",
    "commitlint": "^19.6.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.16"
  },
  "scripts": {
    "prepare": "husky",
    "dev": "npx tailwindcss -i ./css/input.css -o ./css/style.css --watch",
    "build": "npx tailwindcss -i ./css/input.css -o ./css/style.css --minify",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "echo \"No tests specified\" && exit 0",
    "clean": "rimraf node_modules package-lock.json"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.html": "prettier --write",
    "*.md": "prettier --write"
  },
  "engines": {
    "node": ">=14.x",
    "npm": ">=6.x"
  }
}
